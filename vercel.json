{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "serverless/backend/index.py",
      "use": "@vercel/python",
      "config": {
        "maxDuration": 300,
        "memory": 3008
      }
    }
  ],
  "functions": {
    "serverless/backend/index.py": {
      "maxDuration": 300,
      "memory": 3008
    },
    "app/backend/[...path]/route.ts": {
      "maxDuration": 300,
      "memory": 3008
    }
  }
}
